   "source": [
    "import pandas as pd\n",
    "import numpy as np\n",
    "dice = {'1':7,'2':4,'3':1,'4':1}\n",
    "dice_number = list(dice.keys())\n",
    "dice_value =  list(dice.values())\n",
    "dfd = pd.DataFrame(dice_value,index=dice_number,columns=['value'])\n",
    "print('The sum of the dice roll was: {0}'.format(sum(dfd.values)[0]))\n",
    "dfd"
   ]
